#include<bits/stdc++.h>
using namespace std;

bool isPowerOfTwo(int n)
{
    if (n == 0)
        return 0;
    while (n != 1)
    {
        if (n%2 != 0)
            return 0;
        n = n/2;
    }
    return 1;
}

int main()
{
    int x;
    cin>>x;
    if (isPowerOfTwo(x) == 1) cout << "True" << endl;
    else cout << "False" << endl;
  


	return 0;
}

